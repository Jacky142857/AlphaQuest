{
  "name": "alphaquest-frontend",
  "version": 2,
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://alphaquest-t8q4.onrender.com",
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  }
}